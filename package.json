{
	"name": "orderna-api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"sync-schema:docker": "npm run typeorm:docker schema:sync",
		"sync-schema:dev": "npm run typeorm:dev schema:sync",
		"sync-schema:prod": "npm run typeorm:prod schema:sync",
		"start": "npm run sync-schema:dev && cross-env NODE_ENV=development nest start",
		"start:dev": "npm run sync-schema:dev && cross-env NODE_ENV=development nest start --watch",
		"start:prod": "npm run sync-schema:prod && cross-env NODE_ENV=production nest start --watch",
		"start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
		"start:build:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "cross-env NODE_ENV=test jest",
		"test:watch": "cross-env NODE_ENV=test jest --watch --maxWorkers=1",
		"test:cov": "cross-env NODE_ENV=test jest --coverage",
		"test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1 --forceExit",
		"typeorm:docker": "cross-env NODE_ENV=docker ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/configs/typeorm-cli.config.ts",
		"typeorm:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/configs/typeorm-cli.config.ts",
		"typeorm:prod": "cross-env NODE_ENV=production ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/configs/typeorm-cli.config.ts",
		"migration:generate": "npm run typeorm migration:generate",
		"migration:show": "npm run typeorm migration:show",
		"migration:run": "npm run typeorm migration:run",
		"migration:revert": "npm run typeorm migration:revert",
		"migration:create": "typeorm-ts-node-commonjs migration:create"
	},
	"dependencies": {
		"@fastify/secure-session": "^7.5.1",
		"@fastify/static": "^7.0.4",
		"@nest-lab/fastify-multer": "^1.2.0",
		"@nestjs/cache-manager": "^2.2.2",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/common": "^10.3.10",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.3.10",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/mapped-types": "^2.0.5",
		"@nestjs/platform-fastify": "^10.3.8",
		"@nestjs/swagger": "^7.4.0",
		"@nestjs/typeorm": "^10.0.0",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"cache-manager": "^5.7.3",
		"cache-manager-redis-store": "^3.0.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"date-fns": "^3.6.0",
		"dotenv": "^16.4.5",
		"fastify": "^4.28.1",
		"fs": "^0.0.1-security",
		"ioredis": "^5.4.1",
		"jsonwebtoken": "^9.0.2",
		"mime-types": "^2.1.35",
		"moment": "^2.30.1",
		"nest-winston": "^1.9.7",
		"nestjs-form-data": "^1.9.91",
		"path": "^0.12.7",
		"pg": "^8.12.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"sqlite3": "^5.1.6",
		"swagger-ui-express": "^5.0.1",
		"typeorm": "^0.3.17"
	},
	"devDependencies": {
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/jest": "^29.5.2",
		"@types/mime-types": "^2.1.4",
		"@types/moment": "^2.13.0",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^7.13.0",
		"@typescript-eslint/parser": "^7.13.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"jest": "^29.5.0",
		"prettier": "^3.3.2",
		"source-map-support": "^0.5.21",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"moduleDirectories": [
			"node_modules",
			"<rootDir>"
		],
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
